{
  "remoteCache": {
    "apiUrl": "https://turborepo-remote-cache-746116063884.europe-west9.run.app"
  },
  "globalEnv": ["FLIPPABLE_ENV", "USER", "TURBO_TEAM", "GITHUB_HEAD_REF"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "build/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "cache": false
    },
    "ci:lint": {
      "cache": false
    }
  }
}
