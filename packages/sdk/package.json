{
  "name": "@upstart.gg/sdk",
  "version": "0.0.133",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    },
    "./*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    }
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "14.2.0",
    "@date-fns/utc": "1.2.0",
    "@notionhq/client": "4.0.2",
    "@radix-ui/themes": "3.2.1",
    "@rowanmanning/feed-parser": "2.1.0",
    "@sinclair/typebox": "0.34.41",
    "@upstart.gg/style-system": "workspace:*",
    "chroma-js": "3.1.2",
    "date-fns": "3.6.0",
    "debug": "4.3.6",
    "form-data": "4.0.4",
    "get-port": "7.1.0",
    "http-errors-enhanced": "2.0.8",
    "immer": "10.1.1",
    "nanoid": "5.1.5",
    "react-hotkeys-hook": "4.5.1",
    "react-selecto": "1.26.3",
    "usehooks-ts": "3.1.1",
    "zundo": "2.3.0",
    "zustand": "4.5.5"
  },
  "peerDependencies": {
    "@headlessui/react": "^2.2.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "ai": "^5.0.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "lodash-es": "^4.17.21",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240806.0",
    "@types/chroma-js": "3.1.1",
    "@types/cli-progress": "3.11.6",
    "@types/debug": "4.1.12",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^22.15.30",
    "@types/react": "19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/sortablejs": "1.15.8",
    "concurrently": "8.2.2",
    "express": "^4.21.2",
    "rimraf": "6.0.1",
    "tsup": "^8.5.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "lodash-es": "^4.17.21"
  },
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "dev": "tsup --watch",
    "sizetest": "pnpm build && du -sh dist",
    "lint": "biome check --write . && tsc --noEmit",
    "ci:lint": "tsc --noEmit",
    "test": "vitest",
    "gen-ia-docs": "tsx scripts/generate-ia-docs.ts"
  },
  "author": "Matthias Etienne",
  "publishConfig": {
    "access": "public"
  }
}
