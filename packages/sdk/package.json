{
  "name": "@upstart.gg/sdk",
  "version": "0.0.139",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": true,
  "exports": {
    ".": {
      "import": "./dist/shared/index.js",
      "types": "./dist/shared/index.d.ts"
    },
     "./bricks": {
      "types": "./dist/shared/bricks/index.d.ts",
      "import": "./dist/shared/bricks/index.js"
    },
     "./bricks/*": {
      "types": "./dist/shared/bricks/*.d.ts",
      "import": "./dist/shared/bricks/*.js"
    },
     "./ai": {
      "types": "./dist/shared/ai/index.d.ts",
      "import": "./dist/shared/ai/index.js"
    },
    "./analytics": {
      "types": "./dist/shared/analytics/index.d.ts",
      "import": "./dist/shared/analytics/index.js"
    },
    "./datasources": {
      "types": "./dist/shared/datasources/index.d.ts",
      "import": "./dist/shared/datasources/index.js"
    },
    "./datarecords": {
      "types": "./dist/shared/datarecords/index.d.ts",
      "import": "./dist/shared/datarecords/index.js"
    },
    "./site": {
      "types": "./dist/shared/site/index.d.ts",
      "import": "./dist/shared/site/index.js"
    },
    "./themes": {
      "types": "./dist/shared/themes/index.d.ts",
      "import": "./dist/shared/themes/index.js"
    },
    "./utils": {
      "types": "./dist/shared/utils/index.d.ts",
      "import": "./dist/shared/utils/index.js"
    },
    "./shared/*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    },
    "./*": {
      "types": "./dist/shared/*.d.ts",
      "import": "./dist/shared/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/upstart-gg/upstart.git",
    "directory": "packages/sdk"
  },
  "keywords": [
    "upstart",
    "website-builder",
    "builder",
    "ai",
    "react",
    "headless cms",
    "no-code",
    "low-code"
  ],
  "dependencies": {
    "@cfworker/json-schema": "4.1.1",
    "@notionhq/client": "4.0.2",
    "@radix-ui/themes": "3.2.1",
    "@rowanmanning/feed-parser": "2.1.0",
    "@upstart.gg/style-system": "workspace:*",
    "chroma-js": "3.1.2",
    "http-errors-enhanced": "2.0.8",
    "immer": "10.1.1",
    "nanoid": "5.1.5",
    "react-hotkeys-hook": "4.5.1",
    "usehooks-ts": "3.1.1",
    "zundo": "2.3.0",
    "zustand": "4.5.5"
  },
  "peerDependencies": {
    "@headlessui/react": "^2.2.1",
    "@sinclair/typebox": "^0.34.41",
    "ai": "^5.0.0",
    "lodash-es": "^4.17.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20240806.0",
    "@types/bun": "1.2.23",
    "@types/chroma-js": "3.1.1",
    "@types/debug": "4.1.12",
    "@types/express": "^4.17.21",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^24.5.2",
    "@types/react": "19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/sortablejs": "1.15.8",
    "express": "^4.21.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "6.0.1",
    "tsdown": "^0.15.6"
  },
  "scripts": {
    "build": "tsdown",
    "build:analyze": "tsdown --metafile",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "dev": "tsdown --watch",
    "sizetest": "pnpm build && du -sh dist",
    "lint": "biome check --write . && tsc --noEmit",
    "ci:lint": "tsc --noEmit",
    "generate-md-docs": "tsx scripts/generate-md-docs.ts",
    "test": "vitest"
  },
  "author": "Matthias Etienne",
  "publishConfig": {
    "access": "public"
  }
}
